<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="InterfocusConsole"
                   namespace="InterfocusConsole.Entidades">

	<class name="Cliente" table="cliente">
		<id name="Id" type="int">
			<generator class="sequence" >
				<param name="sequence">cliente_seq</param>
			</generator>
		</id>
		<property name="Nome"		type="string"		length="100"		/>
		<property name="Cpf"  type="string"		length="11"	/>
		<property name="Nascimento"	type="date"			not-null="true"	/>
		<property name="Email"		type='string' length='100'		/>
	</class>

</hibernate-mapping>